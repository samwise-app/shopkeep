		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.12",
		"ts-jest": "^28.0.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.0.3",
		"vitest": "^0.19.1"
	},
	"type": "module",
	"typings": "./lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/samwise-app/shopkeep.git"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
